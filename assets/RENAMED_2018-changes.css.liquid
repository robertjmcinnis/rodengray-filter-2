.cart-popup {
    display: none;
  	opacity: 0;
	position: absolute;
    right: 10px;
    top: 50px;
    padding: 20px;
    text-align: center;
    max-width: 140px;
	line-height: 1.4;
  	background: #fff;
    box-shadow: 0px -1px 35px rgba(0,0,0,0.2);
    transition: .2s ease all;
}
.cart-popup:before {
    content: '';
    width: 0;
    height: 0;
    border-left: 10px solid transparent;
    border-right: 10px solid transparent;
    border-bottom: 10px solid #fff;
    position: absolute;
    top: -10px;
    right: 38px;
} 
@media only screen and (max-width: 767px) {
    .cart-popup:before {
        right: 14px;
    }
}
.cart-popup.is-active {
    display: block;
  	opacity: 1;
    transition: .2s ease all;
}
.cart-popup a {
  vertical-align: top;
  margin-right: 0;
  text-decoration: underline;
}


