.shoppable span {
    transition: opacity 0.5s;
    opacity: 0;
    position: absolute;
    width: 20px;
    height: 20px;
    transform: translate(-50%,-50%);
    border: 1px solid transparent;
    background: transparent;
    cursor: pointer;
}
.shoppable span:before {
    content: "";
    display: block;
    width: 19px;
    height: 1px;
    background: #fff;
    margin-top: 8px;
    margin-left:-1px;
}
.shoppable span:after {
    content: "";
    display: block;
    width: 1px;
    height: 19px;
    background: #fff;
    margin-top: -10px;
    margin-left:8px;
}
.shoppable:hover span {
    opacity: 1;
}
